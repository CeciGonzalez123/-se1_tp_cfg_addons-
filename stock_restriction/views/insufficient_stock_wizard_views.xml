<odoo>
    <record id="view_insufficient_stock_wizard_form" model="ir.ui.view">
        <field name="name">insufficient.stock.wizard.form</field>
        <field name="model">insufficient.stock.wizard</field>
        <field name="arch" type="xml">
            <form string="Stock Insuficiente">
                <sheet>
                    <group>
                        <field name="product_id"/>
                        <field name="message"/>
                    </group>
                </sheet>
                <footer>
                    <button string="Cerrar" class="btn-primary" special="cancel" type="object"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

